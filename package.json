    "amqplib": "^0.10.3",
    "elysia": "latest",
    "http-errors": "^2.0.0",
    "lodash": "^4.17.21",
    "pino": "^8.15.1",
    "pino-pretty": "^10.2.0",
    "prisma": "^5.3.1"
  },
  "devDependencies": {
    "@types/http-errors": "^2.0.2",
    "bun-types": "latest"
  },
  "module": "src/main.js",
  "volta": {
    "node": "18.18.0"
  }
